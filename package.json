{
  "name": "ticket-marketplace",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma db push --force-reset && npm run db:seed",
    "db:studio": "prisma studio",
    "db:deploy": "prisma generate --schema=prisma/schema.production.prisma && prisma db push --schema=prisma/schema.production.prisma",
    "migrate:airtable": "tsx scripts/migrate-to-airtable.ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next node_modules prisma/dev.db public/uploads/*",
    "fresh": "npm run clean && npm install && npm run db:push && npm run db:seed"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.11.0",
    "@hookform/resolvers": "^5.1.1",
    "@prisma/client": "^6.10.1",
    "@types/uuid": "^10.0.0",
    "airtable": "^0.12.2",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "lucide-react": "^0.518.0",
    "next": "15.3.4",
    "p-queue": "^8.0.1",
    "prisma": "^6.10.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.58.1",
    "react-hot-toast": "^2.5.2",
    "sharp": "^0.34.2",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/airtable": "^0.10.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "typescript": "^5"
  }
}
